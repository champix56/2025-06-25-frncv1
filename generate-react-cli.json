{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "tsrnfcs": {
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true,
      "customTemplates":{
        "component":"templates/genericTSFunction/TemplateName.tsx",
        "style":"templates/genericTSFunction/TemplateName.styles.js",
        "lazy": "templates/README.md"
      }
    },
    "tsrnfcs-f": {
      "path": "src/components/functional",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true,
      "customTemplates":{
        "component":"templates/genericTSFunction/TemplateName.tsx",
        "style":"templates/genericTSFunction/TemplateName.styles.js",
        "lazy": "templates/README.md"
      }
    }
  }
}